 {
  "compilerOptions": {
    "declaration": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": false,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es6",
    "lib": ["es6"],
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "sourceMap": true
  }
}
